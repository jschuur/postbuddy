{
  "name": "postbuddy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm:dev:site\" \"pnpm:dev:backend\" \"pnpm:db:studio\"",
    "dev:site": "sst bind next dev",
    "dev:backend": "sst dev",
    "dev:backend:prod": "sst dev --stage production",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "update:feeds": "dotenv -e .env.joostschuur tsx src/cli/update.ts",
    "update:feeds:prod": "dotenv -e .env.production tsx src/cli/update.ts",
    "deploy:prod": "sst deploy --stage production",
    "db:push": "dotenv -e .env.joostschuur drizzle-kit push:pg",
    "db:push:prod": "dotenv -e .env.production drizzle-kit push:pg",
    "db:studio": "dotenv -e .env.joostschuur drizzle-kit studio",
    "db:studio:prod": "dotenv -e .env.production drizzle-kit studio"
  },
  "dependencies": {
    "@cloudinary/react": "^1.11.2",
    "@cloudinary/url-gen": "^1.11.2",
    "@hookform/resolvers": "^3.3.1",
    "@radix-ui/react-alert-dialog": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-toast": "^1.1.4",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@tanstack/react-query": "5.0.0-beta.28",
    "@tanstack/react-query-devtools": "5.0.0-beta.28",
    "@tanstack/react-query-next-experimental": "5.0.0-beta.28",
    "@tanstack/react-table": "^8.9.8",
    "@types/node": "20.5.9",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.15",
    "boolean": "^3.2.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.28.6",
    "drizzle-zod": "^0.5.1",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "lodash": "^4.17.21",
    "lucide-react": "^0.274.0",
    "next": "13.4.19",
    "p-limit": "^4.0.0",
    "picocolors": "^1.0.0",
    "pluralize": "^8.0.0",
    "postcss": "8.4.29",
    "postgres": "^3.3.5",
    "pretty-ms": "^8.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.46.1",
    "react-spinners": "^0.13.8",
    "rss-parser": "^3.13.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.2.2",
    "zod": "^3.22.2"
  },
  "devDependencies": {
